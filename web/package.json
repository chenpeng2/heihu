  "scripts": {
    "start": "node --max-old-space-size=8192 scripts/start.js",
    "start:disable-eslint": "npm run start disable-eslint",
    "build": "node --max-old-space-size=8192 scripts/build.js",
    "test": "node scripts/test.js",
    "start-webpackPerformanceCheck": "node scripts/start.js webpackPerformanceCheck",
    "apidoc-watch": "node ./doc/apidoc-watch.js",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ignore-path .eslintignore --ext .jsx --ext .js ./",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "update-schema": "echo hello world",
    "check-commit": "node scripts/check-commit.js",
    "cm": "git-cz",
    "preversion": "git fetch --tags",
    "patch": "npm version patch -m 'chore: upgrade version to %s' && npm run changelog && git add CHANGELOG.md && git commit -m 'chore: update changelog'",
    "postversion": "git push git@gitlab.blacklake.tech:frontend/web.git && git push --tags",
    "changelog": "conventional-changelog -n ./node_modules/conventional-changelog-blacklake -i CHANGELOG.md -s -r 0",
    "auto-translte": "node src/utils/locale/autoTranslate.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --ignore-path .gitignore --ignore-path .eslintignore"
    ],
    "*.jsx": [
      "eslint --ignore-path .gitignore --ignore-path .eslintignore"
    ],
    "*.css": "stylelint",
    "*.scss": "stylelint --syntax=scss",
    "src/configs/env.js": "npm run check-commit"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "globals": {
      "CONF": {
        "API": "http://localhost:9090",
        "BUILD_AT": "2017-06-15T15:22:08+08:00",
        "COMMIT": "0000000000000000000",
        "ENV": "development",
        "VERSION": "66e455b"
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/__mocks__/localStorage.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-blacklake-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+http://gitlab.blacklake.tech/frontend/web.git"
